{
    "compilerOptions": {
        "sourceMap": true,
        "strict": true,
        "target": "es5",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "jsx": "react",
        "noUnusedLocals": true,
        "noEmit": true
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"]
}
